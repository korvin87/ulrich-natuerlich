{
    "name": "abavo/environment",
    "type": "typo3-cms-extension",
    "description": "Configure multiple environments for TYPO3",
    "homepage": "https://github.com/b13/t3ext-environment",
    "license": ["MIT"],
    "keywords": ["TYPO3 CMS", "Environment", "Multi Context"],
    "support": {
        "support": "https://github.com/b13/t3ext-environment"
    },
    "replace": {
        "environment": "self.version"
    },
    "extra": {
		"typo3/cms": {
		  "extension-key": "environment"
		}
	},
    "autoload": {
        "psr-4": {
            "B13\\Environment\\": "Classes/"
         }
    }
}
