{
  "name": "abavo/abavo-maps",
  "type": "typo3-cms-extension",
  "description": "This Extension provides simple GoogleMaps API 3 and OpenStreetMap integration",
  "authors": [
    {
      "name": "Mathias Bruckmoser",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": ">=7.6.0 < 12.0.0"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\AbavoMaps\\": "Classes"
    }
  },
  "extra": {
		"typo3/cms": {
		  "extension-key": "abavo_maps"
		}
	},
  "replace": {
    "abavo_maps": "self.version"
  }
}
